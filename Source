if game.PlaceId == 8992150289 then
    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
    local Window = Library.CreateLib("Anime Sword Simulator", "BloodTheme")
 
    local Main = Window:NewTab("Main")
    local MainSection = Main:NewSection("Main")

    MainSection:NewToggle("Auto Swing", "Taps", function(v)
        getgenv().autoswing = v
        while true do
            if not getgenv().autoswing then return end
            wait(0.2)

local args = {
    [1] = {}
}

game:GetService("ReplicatedStorage").Assets.Events:FindFirstChild("combat attack"):FireServer(unpack(args))
end
end)

MainSection:NewToggle("Auto Train", "Trains - Must be in Training Area", function(v)
    getgenv().autotrain = v
    while true do
        if not getgenv().autotrain then return end

local args = {
    [1] = {
        [1] = workspace.__MAP.Workouts.Training
    }
}

game:GetService("ReplicatedStorage").Assets.Events.training:InvokeServer(unpack(args))

wait(0.1)

end
end)


local Eggs = Window:NewTab("Eggs")
local EggsSection = Eggs:NewSection("Eggs")



local Eggs; --CREATE GLOBAL VARIABLE OF THE USERS SELECTION TO BE USED ANYWHERE
   EggsSection:NewDropdown("Auto Egg Selection", "Click Correct Egg", {"Spawn Egg 1", "Spawn Egg 2", "Dragon Egg 1", "Dragon Egg 2", "Village Egg 1", "Village Egg 2", "Slayer Egg 1", "Slayer Egg 2", "Bizarre Egg 1", "Bizarre Egg 2", "Bliech Egg 1", "Bliech Egg 2", "Hero Egg 1", "Hero Egg 2", "Keisin Egg 1", "Keisin Egg 2",}, function(value)--THIS FUNCTION ALLOWS US TO STORE THE USERS SELECTION
        Eggs = value; --STORES THE USERS SELECTION
    end)
    

