if game.PlaceId == 8992150289 then
    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
    local Window = Library.CreateLib("Anime Sword Simulator", "BloodTheme")
 
    local Main = Window:NewTab("Main")
    local MainSection = Main:NewSection("Main")

    MainSection:NewToggle("Auto Swing", "Taps", function(v)
        getgenv().autoswing = v
        while true do
            if not getgenv().autoswing then return end
            wait(0.2)

local args = {
    [1] = {}
}

game:GetService("ReplicatedStorage").Assets.Events:FindFirstChild("combat attack"):FireServer(unpack(args))
end
end)

MainSection:NewToggle("Auto Train", "Trains - Must be in Training Area", function(v)
    getgenv().autotrain = v
    while true do
        if not getgenv().autotrain then return end

local args = {
    [1] = {
        [1] = workspace.__MAP.Workouts.Training
    }
}

game:GetService("ReplicatedStorage").Assets.Events.training:InvokeServer(unpack(args))

wait(0.1)

end
end)

local Eggs = Window:NewTab("Eggs")
local EggsSection = Eggs:NewSection("Eggs")

EggsSection:NewToggle("Open Spawn Egg", "Opens Egg", function(v)
    getgenv().autoclick = v
    while true do
        if not getgenv().autoclick then return end
local args = {
    [1] = {
        [1] = "spawn-egg-1",
        [2] = false
    }
}
wait(0.5)
game:GetService("ReplicatedStorage").Assets.Events:FindFirstChild("buy egg"):InvokeServer(unpack(args))
end
end)

EggsSection:NewToggle("Open Dragon Egg", "Opens Egg", function(v)
    getgenv().autoclick = v
    while true do
        if not getgenv().autoclick then return end
local args = {
