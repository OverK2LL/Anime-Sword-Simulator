local Boosts = Window:NewTab("Boosts")
local BoostsSection = Boosts:NewSection("Boosts")

BoostsSection:NewButton("Triple Energy", "Uses Boost", function()

local args = {
    [1] = {
        [1] = "Triple Energy Boost"
    }
}

game:GetService("ReplicatedStorage").Assets.Events:FindFirstChild("activate boost"):FireServer(unpack(args))

end)

BoostsSection:NewButton("Triple Gems", "Uses Boost", function()

local args = {
    [1] = {
        [1] = "Triple Gems Boost"
    }
}

game:GetService("ReplicatedStorage").Assets.Events:FindFirstChild("activate boost"):FireServer(unpack(args))

end)

BoostsSection:NewButton("Super Lucky", "Uses Boost", function()

    local args = {
        [1] = {
            [1] = "Super Lucky Boost"
        }
    }
    
    game:GetService("ReplicatedStorage").Assets.Events:FindFirstChild("activate boost"):FireServer(unpack(args))
    
end)

BoostsSection:NewButton("Ultra Lucky", "Uses Boost", function()

local args = {
    [1] = {
        [1] = "Ultra Lucky Boost"
    }
}

game:GetService("ReplicatedStorage").Assets.Events:FindFirstChild("activate boost"):FireServer(unpack(args))

end)

local Misc = Window:NewTab("Misc")
local MiscSection = Misc:NewSection("Misc")

MiscSection:NewSlider("Walkspeed", "Changes the walkspeed", 250, 16, function(v)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v
end)

